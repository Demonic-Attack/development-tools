{
    "name": "@demonicattack/eslint",
    "type": "module",
    "exports": "./dist/index.js",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "pnpm build:clean && pnpm typegen && tsup",
        "build:clean": "rimraf dist",
        "build:inspector": "pnpm build && pnpm dlx @eslint/config-inspector build",
        "clean": "rimraf node_modules",
        "dev": "pnpm typegen && pnpm dlx @eslint/config-inspector --config eslint.config.ts",
        "format": "prettier -w \"**/*.{js,mjs,cjs,jsx,mjsx,ts,tsx,mtsx,json,prisma,md}\"",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "typecheck": "tsc --noEmit",
        "typegen": "typegen scripts/typegen.ts"
    },
    "prettier": "./prettier.config.js",
    "dependencies": {
        "@demonicattack/eslint-plugin": "workspace:*",
        "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
        "@eslint-react/eslint-plugin": "^1.15.0",
        "@eslint/compat": "^1.2.0",
        "@eslint/js": "^9.12.0",
        "@eslint/json": "^0.5.0",
        "@next/eslint-plugin-next": "^14.2.15",
        "@typescript-eslint/eslint-plugin": "^8.9.0",
        "@typescript-eslint/parser": "^8.9.0",
        "eslint": "^9.12.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-eslint": "^11.0.0",
        "eslint-config-flat-gitignore": "^0.3.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-flat-config-utils": "^0.4.0",
        "eslint-import-resolver-typescript": "^3.6.3",
        "eslint-plugin-arca": "^0.16.0",
        "eslint-plugin-better-mutation": "^1.5.0",
        "eslint-plugin-es-x": "^8.0.0",
        "eslint-plugin-eslint-plugin": "^6.2.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jsx-a11y": "^6.10.0",
        "eslint-plugin-n": "^17.11.1",
        "eslint-plugin-no-commented-code": "^1.0.10",
        "eslint-plugin-perfectionist": "^3.9.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-promise": "^7.1.0",
        "eslint-plugin-react": "^7.37.1",
        "eslint-plugin-react-dom": "^1.15.0",
        "eslint-plugin-react-hooks": "^5.1.0-rc-fb9a90fa48-20240614",
        "eslint-plugin-react-hooks-extra": "^1.15.0",
        "eslint-plugin-react-naming-convention": "^1.15.0",
        "eslint-plugin-react-refresh": "^0.4.12",
        "eslint-plugin-react-web-api": "^1.15.0",
        "eslint-plugin-react-x": "^1.15.0",
        "eslint-plugin-regexp": "^2.6.0",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "eslint-plugin-sonarjs": "^2.0.3",
        "eslint-plugin-sort-class-members": "^1.20.0",
        "eslint-plugin-tailwindcss": "^3.17.5",
        "eslint-plugin-unicorn": "^56.0.0",
        "eslint-typegen": "^0.3.2",
        "globals": "^15.11.0",
        "jsonc-eslint-parser": "^2.4.0",
        "local-pkg": "^0.5.0"
    },
    "devDependencies": {
        "@demonicattack/ts": "workspace:*",
