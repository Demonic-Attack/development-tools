{
    "name": "@demonicattack/lint-staged",
    "private": true,
    "type": "module",
    "exports": "./dist/index.js",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "pnpm build:clean && tsup",
        "build:clean": "rimraf dist",
        "clean": "rimraf node_modules",
        "format": "prettier -w \"**/*.{js,mjs,cjs,jsx,mjsx,ts,tsx,mtsx,json,prisma,md}\"",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "typecheck": "tsc --noEmit"
    },
    "prettier": "./prettier.config.js",
    "dependencies": {
        "lint-staged": "^15.2.10",
        "micromatch": "^4.0.8"
    },
    "devDependencies": {
        "@types/micromatch": "^4.0.9",
        "eslint": "^9.12.0",
        "prettier": "^3.3.3",
        "rimraf": "^6.0.1",
        "tsup": "^8.3.0",
        "typescript": "^5.6.3"
    },
    "packageManager": "pnpm@9.12.1"
}
