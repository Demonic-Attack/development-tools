{
    "name": "@demonicattack/typegen",
    "type": "module",
    "exports": "./dist/index.js",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "bin": "./dist/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "pnpm build:clean && tsup",
        "build:clean": "rimraf dist",
        "clean": "rimraf node_modules",
        "format": "prettier --check \"**/*.{js,mjs,cjs,jsx,mjsx,ts,tsx,mtsx,json,prisma,md}\"",
        "format:fix": "prettier --write \"**/*.{js,mjs,cjs,jsx,mjsx,ts,tsx,mtsx,json,prisma,md}\"",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "tsx": "^4.19.1"
    },
    "devDependencies": {
        "eslint": "^9.12.0",
        "prettier": "^3.3.3",
        "rimraf": "^6.0.1",
        "tsup": "^8.3.0",
        "typescript": "^5.6.3"
    },
    "peerDependencies": {
        "tsx": "^4.19.1"
    },
    "packageManager": "pnpm@9.12.1"
}
