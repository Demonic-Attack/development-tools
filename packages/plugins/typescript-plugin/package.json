{
    "name": "@demonicattack/typescript-plugin",
    "version": "0.0.1",
    "description": "",
    "keywords": [],
    "license": "ISC",
    "author": "",
    "type": "module",
    "exports": "./dist/index.js",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "package.json"
    ],
    "scripts": {
        "build": "pnpm build:clean && tsup",
        "build:clean": "rimraf dist",
        "build:inspector": "pnpm build && pnpm dlx @eslint/config-inspector build",
        "clean": "rimraf node_modules",
        "dev": "pnpm dlx @eslint/config-inspector --config eslint.config.ts",
        "format": "prettier -w \"**/*.{js,mjs,cjs,jsx,mjsx,ts,tsx,mtsx,json,prisma,md}\"",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "typecheck": "tsc --noEmit",
        "watch": "tsup --watch"
    },
    "dependencies": {
        "@demonicattack/shared": "workspace:*",
        "@typescript-eslint/utils": "^8.9.0",
        "ts-deepmerge": "^7.0.1"
    },
    "devDependencies": {
        "@demonicattack/prettier": "workspace:*",
        "@demonicattack/ts": "workspace:*",
        "@types/eslint": "^9.6.1",
        "@types/node": "^22.7.5",
        "importx": "^0.5.0",
        "prettier": "^3.3.3",
        "rimraf": "^6.0.1",
        "string-ts": "^2.2.0",
        "tsup": "^8.3.0",
        "typescript": "^5.6.3"
    }
}
