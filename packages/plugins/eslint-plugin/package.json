{
    "name": "@demonicattack/eslint-plugin",
    "version": "0.0.1",
    "description": "",
    "keywords": [],
    "license": "ISC",
    "author": "",
    "type": "module",
    "exports": "./dist/index.js",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "package.json"
    ],
    "scripts": {
        "build": "pnpm build:clean && tsup",
        "build:clean": "rimraf dist",
        "build:inspector": "pnpm build && pnpm dlx @eslint/config-inspector build",
        "clean": "rimraf node_modules",
        "dev": "pnpm dlx @eslint/config-inspector --config eslint.config.ts",
        "format": "prettier -w \"**/*.{js,mjs,cjs,jsx,mjsx,ts,tsx,mtsx,json,prisma,md}\"",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "typecheck": "tsc --noEmit",
        "watch": "tsup --watch"
    },
    "dependencies": {
        "@demonicattack/typescript-plugin": "workspace:*",
        "@demonicattack/shared": "workspace:*"
    },
    "devDependencies": {
        "@demonicattack/eslint": "workspace:*",
        "@demonicattack/prettier": "workspace:*",
        "@demonicattack/ts": "workspace:*",
        "prettier": "^3.3.3",
        "rimraf": "^6.0.1",
        "tsup": "^8.3.0",
        "typescript": "^5.6.3"
    },
    "peerDependencies": {
        "eslint": "^9.12.0",
        "typescript": "^5.6.3"
    }
}
